@model IEnumerable<BestBidEnergy.Models.Supplier> 
@{
    ViewBag.Title = "Auction";
}

<ul class="nav nav-tabs">
    <li role="presentation"><a href="/Suppliers/Index">Home</a></li>
    <li role="presentation"><a href="/Suppliers/Contracts">Contracts</a></li>
    <li role="presentation" class="active"><a href="/Suppliers/Auction">Auction</a></li>
</ul>

<div class="container"> 
     <h2> Supplier Auction</h2> 
     @foreach (var todo in Model){
  <table class="table table-striped">
        <tr> 
            <th class="text-right"> User Name: </th>  
            <td> @todo.UserID/td>
            <th class="text-right"> Monthly kWh Avg: </th>
            <td> -- </td> </tr>
        <tr>
            <th class="text-right"> Credit Score: </th>
            <td> 800 </td>  
            <th class="text-right"> Company: </th> 
            <td>@todo.Company</td></tr>
         <tr>
            <th class="text-right"> Account Number: </th>
            <td> @todo.AccountNum </td>  
            <th class="text-right"> Account Type: </th> 
            <td>commercial</td></tr>
        </table>
        }
      <br><br>
      </div>